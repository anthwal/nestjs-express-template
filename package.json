{
  "name": "main-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "app:command": "ts-node --swc --files src/cli.ts",
    "tinker": "nest start --entryFile tinker"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/sequelize": "^11.0.1",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/terminus": "^11.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "connect-flash": "^0.1.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "nestjs-command": "^3.1.5",
    "nestjs-form-data": "^1.9.93",
    "nestjs-sequelize-paginate": "^1.0.0",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.35.1",
    "sequelize-typescript": "^2.1.6",
    "session-file-store": "^1.5.0",
    "swagger-ui-express": "^5.0.1",
    "ts-enum-util": "^4.1.0",
    "twig": "^1.17.1",
    "umzug": "^3.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^10.2.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-flash": "^0.0.38",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.17.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.4",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/sequelize": "^4.28.20",
    "@types/session-file-store": "^1.2.3",
    "@types/supertest": "^6.0.3",
    "@types/twig": "^1.12.12",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "29.7.0",
    "mockdate": "^3.0.5",
    "nestjs-cls": "^6.2.0",
    "prettier": "^3.7.4",
    "supertest": "^7.2.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.module.(t|j)s",
      "!databases/migrations/*.(t|j)s",
      "!databases/models/**/*.(t|j)s",
      "!databases/config.(t|j)s",
      "!documentation-helper/**/*.(t|j)s",
      "!environment/configs/*.(t|j)s",
      "!main.(t|j)s",
      "!tinker.(t|j)s",
      "!cli.(t|j)s",
      "!migration.(t|j)s",
      "!cli-commands/**/*.(t|j)s",
      "!cluster/**/*.(t|j)s",
      "!helpers/filters/error-validation-format/**/*.(t|j)s",
      "!helpers/utils/check-env-file.(t|j)s",
      "!helpers/interceptors/context/**/*.(t|j)s",
      "!health/controllers/health/**/*.(t|j)s",
      "!common/services/transaction-provider/**/*.(t|j)s",
      "!services/logging/**/*.(t|j)s",
      "!services/url-config-service/**/*.(t|j)s",
      "!mail/services/mail-config/**/*.(t|j)s",
      "!mail/services/mail-config/**/*.(t|j)s",
      "!transaction-manager/services/transaction-provider/**/*.(t|j)s",
      "!view-engine/services/view-engine-config/**/*.(t|j)s",
      "!session-manager/services/session-config/**/*.(t|j)s",
      "!**/*.decorator.(t|j)s",
      "!paginate-overwrite/services/overwrite-paginate.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
